{
  "name": "openclaw-asus-mesh-skill",
  "version": "1.3.0",
  "description": "OpenClaw Skill for ASUS Mesh WiFi Analysis, Optimization and Home Assistant Zigbee Integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./skill": {
      "types": "./dist/skill/openclaw-skill.d.ts",
      "import": "./dist/skill/openclaw-skill.js"
    },
    "./core/mesh": {
      "types": "./dist/core/mesh-analyzer.d.ts",
      "import": "./dist/core/mesh-analyzer.js"
    },
    "./core/zigbee": {
      "types": "./dist/core/zigbee-analyzer.d.ts",
      "import": "./dist/core/zigbee-analyzer.js"
    },
    "./core/triangulation": {
      "types": "./dist/core/real-triangulation.d.ts",
      "import": "./dist/core/real-triangulation.js"
    },
    "./infra/ssh": {
      "types": "./dist/infra/asus-ssh-client.d.ts",
      "import": "./dist/infra/asus-ssh-client.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "openclaw.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:bundle": "node scripts/build.mjs",
    "build:prod": "node scripts/build.mjs --production",
    "build:watch": "node scripts/build.mjs --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "generate:schema": "node scripts/generate-schema.mjs",
    "prepublishOnly": "npm run build && npm run test:run",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\""
  },
  "keywords": [
    "openclaw",
    "asus",
    "mesh",
    "wifi",
    "merlin",
    "home-assistant",
    "zigbee",
    "network-analysis"
  ],
  "author": "OpenClaw Community",
  "homepage": "https://github.com/openclaw/asus-mesh-wifi-analyzer",
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/asus-mesh-wifi-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/openclaw/asus-mesh-wifi-analyzer/issues"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "eventemitter3": "^5.0.1",
    "node-cron": "^3.0.3",
    "pino": "^8.17.0",
    "snmp-native": "^1.2.0",
    "ws": "^8.16.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^1.2.0",
    "esbuild": "^0.27.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
